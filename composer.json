{
  "name": "mile23/virtualpersistapi",
  "description": "The \"Symfony Standard Edition\" distribution",
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "require": {
    "php": ">=5.3.3",
    "leafo/lessphp": "0.4.0",
    "jquery/jquery": "1.9.*",
    "symfony/symfony": "2.3.*",
    "symfony/icu": "1.1.*",
    "doctrine/orm": "*",
    "doctrine/doctrine-bundle": "*",
    "twig/extensions": "1.0.*",
    "symfony/assetic-bundle": "*",
    "sensio/distribution-bundle": "*",
    "sensio/framework-extra-bundle": "*",
    "sensio/generator-bundle": "*",
    "doctrine/data-fixtures": "*",
    "doctrine/doctrine-fixtures-bundle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "phpunit/dbunit": "1.2.*"
  },
  "config": {
    "bin-dir": "bin",
    "secure-http": false
  },
  "minimum-stability": "stable",
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "branch-alias": {
      "dev-master": "2.2-dev"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "jquery/jquery",
        "version": "1.9.1",
        "dist": {
          "url": "http://code.jquery.com/jquery-1.9.1.js",
          "type": "file"
        }
      }
    }
  ]
}
